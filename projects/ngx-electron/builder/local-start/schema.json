{
    "$schema": "http://json-schema.org/schema",
    "title": "Dev Server Target",
    "description": "Dev Server target options for Build Facade.",
    "type": "object",
    "properties": {
        "browserTarget": {
            "type": "string",
            "description": "Target to serve.",
            "pattern": "^[^:\\s]+:[^:\\s]+(:[^\\s]+)?$"
        },
        "electronRoot": {
            "type": "string",
            "description": "electronRoot."
        }
    },
    "additionalProperties": false,
    "required": [
        "browserTarget"
    ]
}
